[user]
  name = Steven Troxler
  email = steven.troxler@gmail.com
[color]
  ui = auto
[core]
  editor = vim
	excludesfile = /Users/steventroxler/.gitignore_global
[merge]
    tool = meld
[alias]
    # log graph
    lg = log --graph --abbrev-commit --decorate --date=relative\
 --format=format:'%C(bold blue)%h%C(reset) - %C(bold\
 green)(%ar)%C(reset) %C(blue)%s%C(reset) -\
 %an%C(reset)%C(bold blue)%d%C(reset)' --all
[url "git@github.com:"]
	insteadOf = https://github.com/
[mergetool]
	prompt = false
